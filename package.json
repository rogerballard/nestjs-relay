{
  "name": "nestjs-relay",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [],
  "main": "dist/nestjs-relay.cjs.js",
  "module": "dist/nestjs-relay.esm.js",
  "typings": "dist/types/nestjs-relay.d.ts",
  "files": [
    "dist"
  ],
  "author": "Roger Ballard <hello@rogerballard.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerballard/nestjs-relay.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run build:docs",
    "build:docs": "typedoc --options .typedoc.js",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.3",
    "@nestjs/core": "^7.1.3",
    "@nestjs/graphql": "^7.4.1",
    "@nestjs/platform-express": "^7.1.3",
    "@nestjs/testing": "^7.1.3",
    "apollo-server-express": "^2.14.2",
    "class-validator": "^0.12.2",
    "graphql": "^15.0.0",
    "graphql-relay": "^0.6.0",
    "graphql-tools": "^6.0.7",
    "reflect-metadata": "^0.1.13",
    "snyk": "^1.342.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/graphql-relay": "0.6.0",
    "@types/jest": "26.0.0",
    "@types/node": "10.17.26",
    "@types/supertest": "2.0.9",
    "colors": "1.4.0",
    "commitizen": "4.1.2",
    "coveralls": "3.1.0",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-config": "23.6.0",
    "lint-staged": "8.2.1",
    "lodash.camelcase": "4.3.0",
    "prettier": "1.19.1",
    "prompt": "1.0.0",
    "replace-in-file": "3.4.4",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.8",
    "shelljs": "0.8.4",
    "supertest": "4.0.2",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "3.9.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
