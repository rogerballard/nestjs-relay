{
  "name": "nestjs-relay",
  "version": "4.0.3",
  "description": "A batteries-included toolkit for building Relay-compliant GraphQL APIs with NestJS",
  "keywords": [
    "nestjs",
    "nestjs-graphql",
    "relay",
    "relay-modern"
  ],
  "main": "dist/cjs/nestjs-relay.js",
  "module": "dist/esm/nestjs-relay.js",
  "typings": "dist/cjs/nestjs-relay.d.ts",
  "files": [
    "dist"
  ],
  "author": "Roger Ballard <hello@rogerballard.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerballard/nestjs-relay.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build:docs": "typedoc --options .typedoc.js --module commonjs",
    "start": "rollup -cw",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --coverage --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/graphql": "^7.7.0",
    "graphql": "^15.4.0",
    "graphql-relay": "^0.6.0",
    "graphql-tools": "^6.2.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/testing": "7.4.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/graphql-relay": "0.6.0",
    "@types/jest": "26.0.14",
    "@types/node": "10.17.35",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "apollo-server-express": "2.18.1",
    "class-validator": "0.12.2",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-config": "26.4.2",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "rollup": "2.28.2",
    "rollup-plugin-typescript2": "0.27.3",
    "semantic-release": "17.2.1",
    "snyk": "1.406.0",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "tsconfig-paths": "3.9.0",
    "typedoc": "0.19.2",
    "typedoc-neo-theme": "1.0.10",
    "typescript": "4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
