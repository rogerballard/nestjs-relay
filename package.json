{
  "name": "nestjs-relay",
  "version": "4.0.4",
  "description": "A batteries-included toolkit for building Relay-compliant GraphQL APIs with NestJS",
  "keywords": [
    "nestjs",
    "nestjs-graphql",
    "relay",
    "relay-modern"
  ],
  "main": "dist/cjs/nestjs-relay.js",
  "module": "dist/esm/nestjs-relay.js",
  "typings": "dist/cjs/nestjs-relay.d.ts",
  "files": [
    "dist"
  ],
  "author": "Roger Ballard <hello@rogerballard.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerballard/nestjs-relay.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build:docs": "typedoc --options .typedoc.js --module commonjs",
    "start": "rollup -cw",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --coverage --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^10.0.11",
    "graphql": "^16.5.0",
    "graphql-relay": "^0.10.0",
    "graphql-tools": "^8.2.9",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/testing": "8.4.4",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/graphql-relay": "0.6.0",
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "apollo-server-express": "3.7.0",
    "class-validator": "0.13.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.8",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.0",
    "jest-config": "28.1.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "rollup": "2.72.1",
    "rollup-plugin-typescript2": "0.31.2",
    "semantic-release": "19.0.2",
    "snyk": "1.924.0",
    "supertest": "6.2.3",
    "ts-jest": "28.0.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
